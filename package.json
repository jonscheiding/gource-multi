{
  "name": "gource-multi",
  "packageManager": "yarn@4.5.1",
  "type": "module",
  "author": {
    "name": "Jon Scheiding",
    "url": "https://github.com/jonscheiding"
  },
  "scripts": {
    "log": "tsc && npx tsx src/build.ts",
