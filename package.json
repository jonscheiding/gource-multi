    "date-fns": "^4.0.0",
    "eslint": "^9.13.0",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^12.1.0",
    "@eslint/js": "^9.13.0",
    "@types/child-process-promise": "^2",
    "@types/ini": "^4.1.1",
    "@types/node": "^22.7.7",
    "globals": "^15.11.0"
